{
  "name": "@narrativegen/web-tester",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run copy:models",
    "copy:models": "pwsh -Command \"Copy-Item -Path '../../models' -Destination 'dist/models' -Recurse -Force\"",
    "preview": "vite preview",
    "lint": "node -e \"console.log('web-tester: lint not configured (skipped)')\""
  },
  "dependencies": {
    "@narrativegen/engine-ts": "file:../../packages/engine-ts",
    "d3": "^7.9.0",
    "dagre": "^0.8.5",
    "mermaid": "^11.12.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.4"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  },
  "devDependencies": {
    "vite": "^5.4.9"
  }
}
